{
  "name": "cv",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "type": "module",
  "main": "build/index.js",
  "module": "build/index.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js --watch",
    "serve": "serve",
    "localize:extract": "lit-localize extract",
    "localize:build": "lit-localize build && eslint --fix 'src/locales/**/*.ts' && prettier 'src/locales' --write",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write"
  },
  "devDependencies": {
    "@lit/localize-tools": "^0.8.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "eslint": "^9.17.0",
    "eslint-config-node": "https://github.com/eetu/eslint-config-node#2.1.0",
    "prettier": "^3.4.2",
    "prettier-config": "https://github.com/eetu/prettier-config#1.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "rollup-plugin-filesize": "^10.0.0",
    "serve": "^14.2.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "dependencies": {
    "@lit/localize": "^0.12.2",
    "lit": "^3.2.1",
    "lit-element": "^4.1.1"
  }
}
